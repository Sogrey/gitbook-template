{
	"title": "GitBook 模板",
	"description": "Gitbook 模板项目",
	"author": "Sogrey",
	"output.name": "site",
	"language": "zh-hans",
	"gitbook": "3.2.3",
	"root": ".",
	"structure": {
		"readme": "README.md"
	},
	"links": {
		"sidebar": {
			"Home": "https://sogrey.github.io/"
		}
	},
	"plugins": [
		"favicon@^0.0.2",
		"donate@^1.0.2"
	],
	"pluginsConfig": {
		"favicon": {
			"shortcut": "favicon.ico",
			"bookmark": "favicon.ico"
		},
		"donate": {
			"wechat": "https://sogrey.github.io/img/wechatPay.png",
			"alipay": "https://sogrey.github.io/img/alipay.jpg",
			"title": "",
			"button": "赏",
			"alipayText": "支付宝打赏",
			"wechatText": "微信打赏"
		}
	},
	"styles": {
		"website": "styles/website.css",
		"ebook": "styles/ebook.css",
		"pdf": "styles/pdf.css",
		"mobi": "styles/mobi.css",
		"epub": "styles/epub.css"
	}
}
